    "@angular/forms": "6.0.3",
    "@angular/http": "6.0.3",
    "@angular/platform-browser": "6.0.3",
    "@angular/platform-browser-dynamic": "6.0.3",
    "@angular/router": "6.0.3",
    "@angular/animations": "6.0.3",
    "@angular/cdk": "6.2.0",
    "@angular/flex-layout": "5.0.0-beta.14",
    "@angular/material": "6.2.0",
    "android-versions": "1.3.0",
    "hammerjs": "2.0.8",
    "core-js": "2.5.7",
    "rxjs": "6.2.0",
    "zone.js": "0.8.26",
    "d3": "5.4.0",
    "@swimlane/ngx-charts": "8.0.2",
    "ng2-charts": "1.6.0",
    "chart.js": "2.7.2",
    "firebase": "5.0.4",
    "node-sass": "4.9.0",
    "signature_pad": "2.3.2",
    "neutrinos-module": "0.0.27",
    "localforage": "1.7.1",
